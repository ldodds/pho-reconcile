{
	"name" : "<%= params[:store] %> Reconciliation Service",
	<% if @opts[:identifierSpace] != nil %>
    "identifierSpace" : "<%= @opts[:identifierSpace] %>",	
	<% else %>
    "identifierSpace" : "<%= @store.storeuri() %>",	
	<% end %>
	<% if @opts[:schemaSpace] != nil %>
    "schemaSpace" : "<%= @opts[:schemaSpace] %>",	
	<% else %>
    "schemaSpace" : "<%= @store.storeuri() %>",	
	<% end %>
	"view" : {
	    "url" : "{{id}}"
	 },
	"_config" : {
	  "search_field" : "<%= @opts[:search_field] %>",
	  <% if @opts[:match_score] != nil %>
	  "match_score" : "<%= @opts[:match_score] %>",
	  <% end %>
	  "store_uri" : "<%= @store.storeuri() %>",
	  "search_service" : "<%= @store.storeuri() %>/items",
	}    
}